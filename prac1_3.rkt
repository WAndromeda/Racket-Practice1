#lang racket

(define (sum_shop_items_price ct kt ck kk)
  (+ (* ct kt) (* ck kk))
)


(printf "НИКОЛАЕВ НИКИТА СЕРГЕЕВИЧ - ИКБО-13-17 - ПРАКТИКА 1 - ЗАДАНИЕ 3\n")
(printf "Вычисление стоимости покупки\n")
(printf "Введите исходные данные:\n")
(printf "Цена тетради (руб.) = ")
(define ct (read))
(printf "Количество тетрадей = ")
(define kt (read))
(printf "Цена карандаша (руб.) = ")
(define ck (read))
(printf "Количество карандашей = ")
(define kk (read))
(printf "Стоимость покупки: ~a руб." (sum_shop_items_price ct kt ck kk) )